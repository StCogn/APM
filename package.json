    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "karma": "^1.7.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "concurrently": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.6",
